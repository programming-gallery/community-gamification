    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "rootDir": ".",
    "outDir": "build",
    "esModuleInterop": true,
    "experimentalDecorators": true, 
    "emitDecoratorMetadata": true ,
    "target": "es6",
    "lib": [
      "esnext"
    ]
  },
  "include": [
    "src/**/*.ts"
  ]
}
